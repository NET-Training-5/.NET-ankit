
<h2>Counting from 1 to 10</h2>
@for (int i = 1; i < 10; i++)
{
    <h3>@i</h3>
}

<h3>Multiple of 3 and 5 less than 50</h3>
@for (int i = 1; i < 50; i++)
{
    if (i%3==0 && i%5==0)
    {
        <h4>@i</h4>
    }
}
<h2>Multiplication Table of 19</h2>
@for (int i = 1; i < 11; i++)
{
    int a= 19;
int output = i * a;
<h3>@a*@i= @output</h3>
}